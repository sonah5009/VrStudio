ngth)+e;d+=" / <"+o+get_title_anu()+" class='anu-raw-ph-no-sec'  ' href='tel:"+s.replace(/[-\s]/g,"")+"'>"+e+"</"+o+">"}),d}}else{if(s=/\(\s*\+\d{1,4}\s*\)\s+\d{2,4}[-]\d{2,4}[-]\d{2,4}\b(?!([-]|\s*\d))/.exec(e)){var l="<"+o+get_title_anu()+" class='anu-raw-ph-no-sec'   href='tel:"+e.replace(/[\(\)\s-]/g,"")+"'>"+e+"</"+o+">";return console.log("s2:",l),l}if(s=/\+\d{1,4}\s+\d{2,4}[-\s]\d{2,4}[-\s]\d{2,4}\b(?!([-]|\s*\d))/.exec(e))return l="<"+o+get_title_anu()+" class='anu-raw-ph-no-sec'   href='tel:"+e.replace(/[\(\)\s-]/g,"")+"'>"+e+"</"+o+">",console.log("s21:",l),l;if(!(s=/\b((0\d{1,4})\s*[-]\s*[1-9][0-9]{5,9})((\s*\/\s*\d{6,11}\b)+)/.exec(e)))return(s=/\b0\d{1,4}\s+\d\d[-\d]{3,8}\d\d/.exec(e))?(console.log("s4"),"<"+o+get_title_anu()+" class='anu-raw-ph-no-sec'   href='tel:"+e.replace(/[-\s]/g,"")+"'>"+e+"</"+o+">"):(s=/\+\d{11,15}\b/.exec(e))?(console.log("s5"),"<"+o+get_title_anu()+" class='anu-raw-ph-no-sec'   href='tel:"+e.replace(/[-\s]/g,"")+"'>"+e+"</"+o+">"):(s=/\+\s*\d{1,4}\s+\d{8,12}\b/.exec(e))?(console.log("s6"),"<"+o+get_title_anu()+" class='anu-raw-ph-no-sec'   href='tel:"+e.replace(/[-\s]/g,"")+"'>"+e+"</"+o+">"):(s=/\s*\b5\d{4}\b(?!([-]|\s*\d))/.exec(e))?(console.log("ret1234=",s),"<"+o+get_title_anu()+" class='anu-raw-ph-no-sec'   href='tel:"+e.replace(/[-\s]/g,"")+"'>"+e+"</"+o+">"):(console.log("s7"),l="<"+o+get_title_anu()+" class='anu-raw-ph-no-sec'   href='tel:"+e.replace(/[-\s]/g,"")+"'>"+e+"</"+o+">");if(console.log("s3"),console.log("ret=",s),s.length>3)return n=s[3],s[2],a=s[1],t=n.split(/\s*\/\s*/).filter(function(e){return e.length>0}),d="<"+o+get_title_anu()+" class='anu-raw-ph-no-sec'   href='tel:"+a.replace(/[-\s]/g,"")+"'>"+a+"</"+o+">",t.forEach(function(e){e=e.trim();var s=a.substring(0,a.length-e.length)+e;d=d+" /  <"+o+get_title_anu()+" class='anu-raw-ph-no-sec'   href='tel:"+s.replace(/[-\s]/g,"")+"'>"+e+"</"+o+">"}),d}}),e(s).replaceWith(n)}else console.log("sel obj not found")}
;
"use script";



function anu_sticky_headers($)
    {
        $(".node table").addClass('sticky-header');

        var all_tables = $(".node table").filter(function ()
            {
                //if th is in first tr and has more than 10 rows of tr
                return   $(this).find('tr:first >th').length && ($(this).find('tr').length > 10);
            });


        all_tables.each(function ()
            {
                if ($(this).find('thead').length == 0)
                    {
                        var parent = $($(this).find('th')[0]).parent();

                        if (parent.is('tr'))
                            {
                                //     parent.css('border', '5px solid red');
                                //wrap the in thead
                                parent.wrap('<thead>   </thead>');

                                parent.parent().prependTo($(this));
                            }
                        else
                            {
                                //skip this table
                            }
                    }
            });

        anuFloatThread($);

        var $table = $('.sticky-header');
        $table.floatThead();
    }

function make_tables_searcheable($)
    {
        //add a filter element inthe top of every table
        var id = 1;
        $(".node table").each(function ()
            {
	    if(  $(this).find('tr').length>6 ){
	    $("<div title='Type in anything to filter the table data' class='tblsrcdiv'><label class='tblsrclabel'>Table Search:<input class='tblsrc' id='atblsrc" + id +
		    "' placeholder='Search text' type='text'> </label></div>").insertBefore($(this));
	    $(this).addClass('ntblesr' + id);
	    ++id;
	    }
	    });



        $(".tblsrc").on("keyup", function ()
            {
                var value = $(this).val();

                console.log("value=", value);
                
                if(value.trim().length>0)
                {
                    $(this).addClass('   tablefiltertypedtext'  );                   
                }
                else
                {
                     $(this).removeClass('anublink   tablefiltertypedtext');
                }

                var id = $(this).attr('id');
                
                console.log("id=",id);                

                var number = id.replace(/[^\d]/g, '');

                console.log("sel=", ".ntblesr" + number + ' tr');
                $(".ntblesr" + number + ' tr').each(function (index)
                    {
                        console.log("index=", index);
                                        $(this).show();
                        if (index != 0)
                            {
                                $row = $(this);

                                var text = $row.text().replace(/\s\s+/g,' ');
                                
                                console.log("row text=",text);

				console.log("searching value=",value," cmp=",text.toLowerCase().trim().indexOf(value.toLowerCase().trim()));

                                if (text.toLowerCase().trim().indexOf(value.toLowerCase().trim()) <0)
                                    {
                                        console.log("hidden=",$(this).text());
                                        $(this).hide();
                                    }
                                else
                                    {
                                        console.log("shown=",$(this).text());
                                        $(this).show();
                                    }
                            }
                    });
            });
    }
/*
