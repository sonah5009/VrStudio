var anu_main_fun = function ()
    {
        anu_check_mobile_numbers(jq331);

        setTimeout(function ()
            {
//                anu_sticky_headers(jq331);
            }, 4000);

        make_tables_searcheable(jq331);

    }

$(document).ready(anu_main_fun);

